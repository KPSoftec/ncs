{
  "name": "react_boiler_plate",
  "version": "1.2.0",
  "description": "A Framework/Boiler-Plate Developed In REACT",
  "keywords": [
    "React",
    "Boiler-Plate",
    "Framework"
  ],
  "author": {
    "name": "Karan Khilwani",
    "email": "khilwanikaran@gmail.com"
  },
  "contributors": [
    {
      "name": "Karan Khilwani",
      "email": "khilwanikaran@gmail.com"
    }
  ],
  "license": "ISC",
  "scripts": {
    "analyze": "source-map-explorer build/js/*.js",
    "start": "npm-run-all -p -r mb:start dev",
    "start:vagrant": "npm-run-all -s build:vagrant serve:build",
    "start:wp:dev:server": "cross-env BUILD_DOMAIN=devBuild run-p -r mb:start wp:dev:server",
    "wp:dev:server": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "bs:start": "cross-env NODE_ENV=development browser-sync start --config ./config/browser-sync/bs-config.js",
    "bs:start:old": "cross-env NODE_ENV=development && node ./config/browser-sync",
    "build": "npm-run-all clean:build prod",
    "build:dev": "cross-env NODE_ENV=development npm-run-all build",
    "build:prod": "cross-env NODE_ENV=production npm-run-all build",
    "build:vagrant": "cross-env NODE_ENV=production npm-run-all build",
    "check:package:outdated": "yarn outdated",
    "clean:all": "run-p clean:dll && clean:build && clean:nm",
    "clean:build": "rimraf build",
    "clean:dll": "rimraf dll",
    "clean:nm": "rimraf node_modules",
    "commitmsg": "cz-customizable-ghooks",
    "cz": "git-cz",
    "dev": "cross-env BUILD_DOMAIN=devBuild npm-run-all bs:start",
    "dev:vagrant": "cross-env BUILD_DOMAIN=vagrant npm-run-all bs:start",
    "dev:build": "cross-env BUILD_DOMAIN=devBuild npm-run-all build:dev",
    "development:build": "cross-env BUILD_DOMAIN=development run-s build:dev",
    "dll": "cross-env NODE_ENV=dll webpack",
    "e2e": "npm-run-all -p -r serve:build:e2e mb:start system:test:delay:start",
    "e2e:dev": "npm-run-all -p -r serve:build:e2e mb:start system:test:sele:dev",
    "e2e:build": "npm-run-all dev:build e2e",
    "e2e:grid": "cross-env BUILD_DOMAIN=development TEST_DOMAIN=ci-local npm-run-all system:test",
    "e2e:grid:report": "cross-env BUILD_DOMAIN=development TEST_DOMAIN=ci-server npm-run-all system:test:report",
    "e2e:grid:server": "cross-env TEST_DOMAIN=ci-server npm-run-all system:test",
    "format": "npm-run-all -p format:*",
    "format:scripts": "npm run lint:scripts -- --fix",
    "format:styles": "stylelint \"./src/app/**/**/styles.js\" --fix",
    "fre-ci": "npm-run-all lint test:update:snapshot fre-ci:build fre-e2e fre-ci:create-artifacts",
    "fre-ci:dev": "npm-run-all lint test:update:snapshot fre-ci:build fre-e2e:dev",
    "fre-noci:dev": "npm-run-all lint test:update:snapshot fre-ci:build fre-e2e:noci:dev",
    "fre-ci:release": "npm-run-all lint test:update:snapshot fre-ci:build fre-e2e fre-ci:semantic-release",
    "fre-ci:release:no-e2e": "npm-run-all lint test:update:snapshot fre-ci:build fre-ci:semantic-release",
    "fre-ci:build": "npm-run-all prod:dev:build",
    "fre-ci:no-e2e": "npm-run-all lint test:update:snapshot prod:build fre-ci:create-artifacts",
    "fre-ci:semantic-release": "corp-semantic-release --changelogpreset angular-bitbucket -b release/react-boiler-plate --ci-skip false --post-success \"npm run fre-ci:post-success\"",
    "fre-ci:create-artifacts": "npm run fre-ci:post-success",
    "fre-ci:post-success": "npm-run-all release:build fre-ci:success-vars fre-ci:bundle:release",
    "fre-ci:success-vars": "./ci-scripts/success-vars.sh",
    "fre-ci:bundle:release": "./ci-scripts/bundle-release.sh",
    "fre-ci:shutdown": "npm-run-all fre-ci:bundle:screenshots fre-ci:sonarqube",
    "fre-ci:bundle:screenshots": "./ci-scripts/bundle-screenshots.sh",
    "fre-e2e": "cross-env BUILD_DOMAIN=test TEST_DOMAIN=ci npm-run-all -p e2e",
    "fre-e2e:dev": "cross-env BUILD_DOMAIN=test TEST_DOMAIN=ci npm-run-all -p system:test:sele:launch e2e:dev",
    "fre-e2e:noci:dev": "cross-env BUILD_DOMAIN=test TEST_DOMAIN=noci npm-run-all -p system:test:sele:launch e2e:dev",
    "fre-copy-driver": "./ci-scripts/copy-drivers.sh",
    "fre-ci:sonarqube": "./ci-scripts/sonarqube.sh",
    "in:sele": "npm-run-all -s sele:install",
    "in:sele:dev": "npm-run-all -s sele:install sele:start",
    "init:bs": "browser-sync init",
    "lint": "npm-run-all -p lint:*",
    "lint:styles": "stylelint \"src/app/**/*.js\"",
    "lint:scripts": "eslint \"src/app/**/*.js\" \"src/app/**/*.jsx\" \"config/**/*.js\"",
    "mb": "node ./node_modules/mountebank/bin/mb --loglevel error --allowInjection true",
    "mb:start": "run-p mb mb:configure:delay",
    "mb:configure": "node ./config/mountebank/config.js",
    "mb:configure:delay": "sleep 1 && npm run mb:configure",
    "precommit": "run-s precommit:*",
    "precommit:verify": "npm run lint",
    "precommit:test": "npm run test",
    "precommit:build": "npm run prod:build",
    "prepush": "run-s prepush:*",
    "prepush:verify": "npm run lint",
    "prepush-disabled:test": "npm run test",
    "prepush-disabled:build": "npm run prod:build",
    "prod": "webpack",
    "prod:dev:build": "cross-env BUILD_DOMAIN=devBuild run-s build:prod",
    "prod:build": "cross-env BUILD_DOMAIN=production run-s build:prod",
    "vagrant:build": "cross-env BUILD_DOMAIN=vagrant run-s build:vagrant",
    "release:build": "cross-env BUILD_DOMAIN=release run-s build:prod",
    "report": "node_modules/.bin/allure generate reports/systemTest/allure-results/ -o reports/systemTest/allure-reports/ --clean",
    "reports:clean": "rimraf reports/systemTest",
    "sele:install": "./node_modules/.bin/selenium-standalone install --config=./config/selenium-standalone/selenium-standalone-config.js",
    "sele:start": "./node_modules/.bin/selenium-standalone start --config=./config/selenium-standalone/selenium-standalone-config.js",
    "serve:allure-reports": "serve ./reports/systemTest/allure-reports -l 9091",
    "serve:build": "serve ./build",
    "serve:build:stubs": "npm-run-all prod:dev:build serve:stubs",
    "serve:build:mock": "npm-run-all prod:dev:build serve:mock",
    "serve:build:mock:vag": "npm-run-all vagrant:build serve:mock",
    "serve:build:e2e": "cross-env BUILD_DOMAIN=test run-s serve:static",
    "serve:mock": "run-p mb:start serve:build:e2e serve:selenium",
    "serve:static": "node ./test/serve-static.js",
    "serve:selenium": "serve ./selenium-drivers -l 9090",
    "serve:stubs": "cross-env BUILD_DOMAIN=test TEST_DOMAIN=automation serve:static",
    "system:test": "wdio './config/wdio/index.js'; wdio_ret=$?; exit $wdio_ret",
    "system:test:sele": "npm-run-all -p in:sele system:test:delay:start",
    "system:test:sele:dev": "npm-run-all -p in:sele:dev system:test:delay:start",
    "system:test:sele:launch": "npm-run-all -p serve:selenium in:sele:dev",
    "system:test:delay:remote:start": "sleep 5 && cross-env TEST_DOMAIN=remote npm run system:test",
    "system:test:delay:start": "sleep 5 && npm run system:test",
    "system:test:report": "npm-run-all -c reports:clean system:test report",
    "test": "cross-env NODE_ENV=unittest jest --config config/jest/config.json --runInBand --detectOpenHandles",
    "test:update:snapshot": "npm run test -- --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "update:package": "ncu -u",
    "updgrade:package": "yarn upgrade --latest",
    "update:cache": "yarn cache clean && rimraf yarn.lock npm-packages-cache node_modules && yarn updgrade:package",
    "yarn:outdated": "yarn outdated"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "lodash": "^4.17.20",
    "mobile-detect": "^1.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "react-styled-flexboxgrid": "^3.2.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@types/react": "^16.9.35",
    "@wdio/allure-reporter": "^6.1.14",
    "@wdio/cli": "^6.1.17",
    "@wdio/config": "^6.1.14",
    "@wdio/cucumber-framework": "^6.1.17",
    "@wdio/dot-reporter": "^6.1.14",
    "@wdio/junit-reporter": "^6.1.14",
    "@wdio/local-runner": "^6.1.17",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "@wdio/spec-reporter": "^6.1.14",
    "@wdio/sync": "^6.1.14",
    "allure-commandline": "^2.13.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^6.0.2",
    "corp-semantic-release": "^7.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "csvtojson": "^2.0.10",
    "cucumber": "^6.0.5",
    "cz-customizable": "^6.2.0",
    "cz-customizable-ghooks": "^1.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "http-proxy-middleware": "^1.0.4",
    "husky": "^4.2.5",
    "ip": "^1.1.5",
    "jest": "^26.0.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-junit": "^10.0.0",
    "jest-matchmedia-mock": "^1.0.1",
    "jest-sonar": "^0.2.9",
    "jest-sonar-reporter": "^2.0.0",
    "mountebank": "^2.2.1",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "npm-check-updates": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "selenium-server": "^3.141.59",
    "selenium-standalone": "^6.17.0",
    "serve": "^11.3.2",
    "serve-static": "^1.14.1",
    "sonar-scanner": "^3.1.0",
    "source-map-explorer": "^2.5.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^3.9.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "wdio-cucumberjs-json-reporter": "^2.0.2",
    "wdio-intercept-service": "^4.1.2",
    "wdio-json-reporter": "^2.0.0",
    "webdriverio": "^6.1.17",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "yargs": "^15.3.1"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/cz-customizable/config.js"
    }
  }
}
